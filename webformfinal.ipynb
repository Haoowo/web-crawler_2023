   "source": [
    "#插入資料\n",
    "insert_sql = \"INSERT INTO cp1500 (name, moves, score) VALUES (?, ?, ?)\"\n",
    "\n",
    "for row in table_rows:\n",
    "    values = (row['name'], row['moves'], row['score'])\n",
    "    cursor.execute(insert_sql, values)\n",
    "\n",
    "# 提交資料庫交易\n",
    "conn.commit()\n"
   ]
