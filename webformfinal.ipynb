    "from flask import Flask, render_template\n",
    "app = Flask(__name__)\n",
    "\n",
    "table_rows = []\n",
    "@app.route('/')\n",
    "def display_table():\n",
    "\n",
    "    \n",
    "    # 每三項為一組，以並排顯示\n",
    "    for i in range(0, len(text), 3):\n",
    "        name = text[i]\n",
    "        moves = text[i + 1]\n",
    "        \n",
    "        score = text[i + 2]\n",
    "        \n",
    "        \n",
    "        table_rows.append({\n",
    "            'name': name,\n",
    "            'moves': moves,\n",
    "            'score': score\n",
    "        })\n",
    "\n",
    "    return render_template('index.html', rows=table_rows)\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    app.run()\n"
   ]
  },
